	<a class="{{if .PageIsExploreOrganizations}}active{{end}} item" href="{{AppSubUrl}}/explore/organizations">
		<span class="octicon octicon-organization"></span> {{.i18n.Tr "explore.organizations"}}
	</a>
	{{if .IsRepoIndexerEnabled}}
